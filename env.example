# Martina Schedule Bot Environment Variables
# Copy this file to .env and fill in your values

# Bot Tokens (get from @BotFather on Telegram)
BOT_TOKEN=your_admin_bot_token_here
STAFF_BOT_TOKEN=your_staff_bot_token_here

# Admin User IDs (comma-separated)
ADMIN_IDS=123456789,987654321

# Database Path
DATABASE_PATH=/opt/martina-bot/shared_scheduler.db

# Optional: Web Server Port
PORT=10000 